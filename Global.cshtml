<?xml version="1.0" encoding="utf-8"?>
<timeline>
  <name>グローバルトリガ</name>
  <rev></rev>
  <description>
    ゾーンに依存せず常に使用するトリガを定義する。
  </description>
  <zone>{GLOBAL}</zone>
  <locale>JA</locale>

  <!-- 初期設定では無効にしてあります -->
  <!--
  <default target-element="Trigger" target-attr="enabled" value="false" />
  -->

<!-- TL音声 -->
  <default target-element="Activity" target-attr="notice-d" value="Main" />
<!-- トリガー音声 -->
  <default target-element="Trigger" target-attr="notice-d" value="Main" />
<!-- 音声通知何秒前 -->
  <default target-element="Activity" target-attr="notice-o" value="-3" />
<!-- ログの通知間隔 -->
  <default target-element="Activity" target-attr="sync-s" value="-5" />
  <default target-element="Activity" target-attr="sync-e" value="5" />

  <!-- 成功テスト用 -->
<t name="初期配置" sync="" exec="scripts\ppas.ps1" args="" exec-hidden="true" />

<t
  name="テスト"
  sync=""
  exec="POST https://localhost:1337/preset//load" />

  <!-- 見本 -->
<t name="初期配置" sync="マーカーせぇええええっとおお" exec="scripts\ppas.ps1" args="α4層前半" exec-hidden="true" />
<t name="安置解除" sync="" exec="scripts\ppoff.ps1" args="" exec-hidden="true" />

<t text="マーカー\n ➜ ${_mex}" sync="1B:[id8]:[mex]:" notice="">
  <v-notice
    duration="5"
    duration-visible="false"
    job-icon="true" />
</t>



  <t text="北" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.50 Y=13.11" notice="北">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="北東" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.78 Y=13.22" notice="北東">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="東" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.89 Y=13.50" notice="東">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="南東" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.78 Y=13.78" notice="南東">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="南" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.50 Y=13.89" notice="南">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="南西" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.22 Y=13.78" notice="南西">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="西" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.11 Y=13.50" notice="西">
    <v-notice duration="6" order="-5"/>
  </t>
  <t text="北西" sync="\[EX\] Added new combatant. name=ミストドラゴン X=15.22 Y=13.22" notice="北西">
    <v-notice duration="6" order="-5"/>
  </t>

  <t text="３の倍数\n ➜ 3, 6, 9, 12, 15..." sync="「3の倍数」ニ　調整シテクダサイ！" notice="3の倍数。">
    <v-notice 
      duration="6" 
      duration-visible="false"
      order="-5" />
  </t>

  <t text="４の倍数\n ➜ 4, 8, 12, 20..." sync="「4の倍数」ニ　調整シテクダサイ！" notice="4の倍数。">
    <v-notice 
      duration="6" 
      duration-visible="false"
      order="-5" />
  </t>

  <t text="５の倍数\n ➜ 5, 10, 15..." sync="「5の倍数」ニ　調整シテクダサイ！" notice="5の倍数。">
    <v-notice 
      duration="6" 
      duration-visible="false"
      order="-5" />
  </t>

  <t text="素数\n ➜ 1, 2, 3, 5, 7, 11, 13..." sync="「素数」ニ　調整シテクダサイ！" notice="素数。">
    <v-notice 
      duration="6" 
      duration-visible="false"
      order="-5" />
  </t>

  <t text="➖ マイナス ➖" sync="[mex]に「磁場崩壊【－】」の効果。" notice="マイナス。">
    <v-notice 
      duration="4" 
      order="-5" />
  </t>

  <t text="➕ プラス ➕" sync="[mex]に「磁場崩壊【＋】」の効果。" notice="プラス。">
    <v-notice 
      duration="4" 
      order="-5" />
  </t>


    <t text="魔法障壁\n ➜ ${_pc}" sync="[pc]に「魔法障壁」の効果。">
      <v-notice
        duration="90"
        sync-to-hide="${_pc}の「魔法障壁」が切れた。"
        order="-3"
        icon="注意.png" />
    </t>

    <t text="レゾナンス：ニアー\n ➜ 離す"  sync="^1A:オメガ gains the effect of レゾナンスプログラム：ニアー from  for 50.00 Seconds. " notice="離して離してえ">
      <v-notice duration="8" icon="Pair" duration-visible="false" />
    </t>

    <t text="レゾナンス：ファー\n ➜ 近づける"  sync="^1A:オメガ gains the effect of レゾナンスプログラム：ファー from  for 50.00 Seconds." notice="近づけて近づけてえ">
      <v-notice duration="8" icon="Pair" duration-visible="false" />
    </t>

 -<t sync="[mex] gains the effect of 低気圧 from ガルーダ" text="低気圧">
   <v-notice order="15" duration-visible="false" duration="10" stack-visible="true" sync-to-hide="[mex]に「高気圧」の効果。"/>
   </t>

  <t notice="低気圧。スタック1" sync="increments stacks of 低気圧 to 1."/>
  <t notice="低気圧。スタック2" sync="increments stacks of 低気圧 to 2."/>
  <t notice="低気圧。スタック3" sync="increments stacks of 低気圧 to 3."/>

<t text="ダウンバースト" sync="ガルーダの「ダウンバースト」" notice="ダウンバースト来るよ！">
      <v-notice 
        duration="3" 
        icon="LB.png" />
    </t>

 <t text="ツイスター" sync="ツインタニアは「ツイスター」の構え。" notice="ツイスター来るよ！" enabled="true">
    <v-notice
      duration="3"
      duration-visible="true"
      icon="ツイスター.png" />
  </t>

 <t text="テストトリガー" sync="てすと" notice="テストトリガー発動中" enabled="true">
    <v-notice
      duration="5"
      duration-visible="true"
      icon="LB.png" />
<t 
  name="" 
  sync="てすと" 
  exec="scripts\set_waymarks_sample.ps1" 
  exec-hidden="true" />
  </t>

</timeline>
